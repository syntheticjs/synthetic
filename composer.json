{
    "name": "syntheticjs/laravel-synthetic",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Caleb Porzio",
            "email": "caleb@calebporzio.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.22",
        "illuminate/database": "^9.22",
        "illuminate/validation": "^9.22"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4|^9.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Synthetic\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Synthetic\\SyntheticServiceProvider"
            ],
            "aliases": {
                "Synthetic": "Synthetic\\SyntheticFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
